<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="234.0" prefWidth="303.0" spacing="10" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.FormController">
    <children>
        <Label text="Usuario o Email" />
        <TextField fx:id="nombreField" promptText="Ingrese su usuario o email" />
        <Label text="Contraseña" />
        <TextField fx:id="contraseñaField" promptText="Ingrese su Contraseña" />
      <Button mnemonicParsing="false" onAction="#iniciarSesion" prefHeight="26.0" prefWidth="108.0" text="Iniciar Sesion" />
        <Button onAction="#abrirRegister" text="Registrar Usuario" />
      <Label text="Desea crear un nuevo Usuario?" />
    </children>
</VBox>
